{
  "Schematics": {
    "user": {
      "name": {
        "method": "humanName",
        "gender": "__genre"
      },
      "age": {
        "method": "number",
        "numberType": "int",
        "start": "10",
        "stop": 40
      },
      "genre": {
        "method": "choice",
        "data": ["male", "female"]
      },
      "card": {
        "method": "chooseSeveral",
        "data": ["Café", "Açucar", "Sal","Pimenta"],
        "repeat": false
      }
    },
    "product": {
      "price": {
        "method": "number",
        "start": 10,
        "stop": 90,
        "precision": "__precision",
        "transform": "currency"
      },
      "name": {
        "method": "choice",
        "data": "teste.txt",
        "repeat": false
      },
      "Due date": {
        "method": "date",
        "dateType": "future"
      },
      "precision": {
        "method":"choice",
        "data": [2,3,4]
      }
    }
  },
  "DataBase": {
      "users":{
        "schema":"user",
        "count": 10
      },
      "products": {
        "schema": "product",
        "count": 3
      }
  }
}
