{
  "Schematics": {
    "user": {
      "name": {
        "method": "humanName",
        "gender": "__genre"
      },
      "age": {
        "method": "sequence",
        "data":[1,2,3]
      },
      "randomSequence": {
        "method": "randomSequence",
        "data":[1,2,3,4,5,6,7,8],
        "size":4
      },
      "genre": {
        "method": "choice",
        "data": ["male", "female"]
      },
      "card": {
        "method": "chooseSeveral",
        "data": "@products:None:precision>2:all@",
        "repeat": false
      }
    },
    "product": {
      "price": {
        "method": "number",
        "start": 10,
        "stop": 90,
        "precision": "__precision",
        "transform": "currency"
      },
      "name": {
        "method": "choice",
        "data": "teste.txt",
        "repeat": false
      },
      "Due date": {
        "method": "date",
        "dateType": "future"
      },
      "precision": {
        "method":"choice",
        "data": [2,3,4]
      }
    }
  },
  "DataBase": {
      "users":{
        "schema":"user",
        "size": 10
      },
      "products":  "product"
  }
}
